<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>materialtype</servlet-name>
        <servlet-class>servlet.materialtype</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>materialcategoury</servlet-name>
        <servlet-class>servlet.materialcategoury</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>itemdescriptiontype</servlet-name>
        <servlet-class>servlet.itemdescriptiontype</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gener</servlet-name>
        <servlet-class>servlet.gener</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>unit</servlet-name>
        <servlet-class>servlet.unit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>itemdescription</servlet-name>
        <servlet-class>servlet.itemdescription</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>materialtype</servlet-name>
        <url-pattern>/materialtype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>materialcategoury</servlet-name>
        <url-pattern>/materialcategoury</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>itemdescriptiontype</servlet-name>
        <url-pattern>/itemdescriptiontype</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gener</servlet-name>
        <url-pattern>/gener</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>unit</servlet-name>
        <url-pattern>/unit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>itemdescription</servlet-name>
        <url-pattern>/itemdescription</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
